
 <main id='users-index'>
  <% @users.each do |user| %>
      
      <p>
        Name: <%= user.name%><br/>
      </p>
      <%= link_to(user.email, user) %> 
  <% end %>



  <p>
    <%= link_to('Create User', new_user_path)%>
  </p>

  <p>
    <%= link_to('Create Post', new_post_path)%>
  </p>

  <% if session[:user_id] %>
     hi you are logged in!
    <%= link_to 'Signout', destroy_session_path%>
  <% else %>
    <%= link_to 'Signin', new_session_path %>
  <% end %>

</main>




